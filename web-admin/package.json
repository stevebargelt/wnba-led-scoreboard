{
  "name": "wnba-led-web-admin",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "npm run pretest:ci && jest --coverage --watchAll=false",
    "pretest:ci": "(npm install --no-save @next/swc-linux-x64-gnu || true) && if command -v ldd >/dev/null 2>&1 && ldd --version 2>&1 | grep -qi musl; then npm install --no-save @next/swc-linux-x64-musl || true; fi",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.1",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "ajv": "^8.12.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "next": "^14.2.5",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "19.1.12",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.2.32",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "typescript": "5.9.2"
  }
}
